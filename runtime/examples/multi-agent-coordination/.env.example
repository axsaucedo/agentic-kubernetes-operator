# Multi-Agent Coordination Example - Local Development

# Ollama model API (shared across all agents)
MODEL_API_URL=http://localhost:11434/v1
MODEL_API_KEY=ollama

# Coordinator Agent
COORDINATOR_NAME=coordinator
COORDINATOR_DESCRIPTION=Orchestrates tasks between specialized agents
COORDINATOR_PORT=8000
COORDINATOR_LOG_LEVEL=INFO

# Researcher Agent
RESEARCHER_NAME=researcher
RESEARCHER_DESCRIPTION=Gathers and analyzes information
RESEARCHER_PORT=8001
RESEARCHER_LOG_LEVEL=INFO

# Analyst Agent
ANALYST_NAME=analyst
ANALYST_DESCRIPTION=Performs calculations and analysis
ANALYST_PORT=8002
ANALYST_LOG_LEVEL=INFO
ANALYST_MCP_SERVERS=math-tools
ANALYST_MCP_SERVER_MATH_TOOLS_URL=http://localhost:8003

# MCP Server for math tools
MCP_MATH_TOOLS_PORT=8003

# A2A Communication Setup
# Format: agent_name:http://host:port/agent/card
COORDINATOR_PEER_AGENTS=researcher:http://localhost:8001/agent/card,analyst:http://localhost:8002/agent/card
RESEARCHER_PEER_AGENTS=coordinator:http://localhost:8000/agent/card
ANALYST_PEER_AGENTS=coordinator:http://localhost:8000/agent/card
